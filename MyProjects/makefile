# User makefile generated by OptFrame MCT (Make a Compilable Thing!)

all: mctAppODBFT

mctAppODBFT:      mainODBFT.o  ODBFTProblemInstance.o  ODBFTEvaluator.o ODBFTConstructiveRandom.o  ODBFTNSSeqChangeNode.o   ODBFTScanner.o  
	g++ $(GCC_FLAGS) mainODBFT.o  ODBFTProblemInstance.o  ODBFTEvaluator.o ODBFTConstructiveRandom.o  ODBFTNSSeqChangeNode.o   ODBFTScanner.o -o app_ODBFT

mainODBFT.o: mainODBFT.cpp  ODBFT/Representation.h 
	g++ -c $(GCC_FLAGS) mainODBFT.cpp -o mainODBFT.o
	
ODBFTProblemInstance.o: ODBFT/ProblemInstance.cpp ODBFT/ProblemInstance.h
	g++ -c $(GCC_FLAGS) ODBFT/ProblemInstance.cpp -o ODBFTProblemInstance.o
	
ODBFTEvaluator.o: ODBFT/Evaluator.cpp ODBFT/Evaluator.h
	g++ -c $(GCC_FLAGS) ODBFT/Evaluator.cpp -o ODBFTEvaluator.o
	
ODBFTConstructiveRandom.o: ODBFT/ConstructiveRandom.cpp ODBFT/ConstructiveRandom.h
	g++ -c $(GCC_FLAGS) ODBFT/ConstructiveRandom.cpp -o ODBFTConstructiveRandom.o

ODBFTNSSeqChangeNode.o: ODBFT/NSSeqChangeNode.cpp ODBFT/NSSeqChangeNode.h
	g++ -c $(GCC_FLAGS) ODBFT/NSSeqChangeNode.cpp -o ODBFTNSSeqChangeNode.o
	
ODBFTScanner.o: ../OptFrame/Scanner++/Scanner.cpp
	g++ -c $(GCC_FLAGS) ../OptFrame/Scanner++/Scanner.cpp -o ODBFTScanner.o



GCC_FLAGS=-Ofast --std=c++1z -Wall

examples:
	make -C ../Examples/
	
clean: 
	make clean -C ../Examples/
	rm -f app_*
	rm -f *.o
